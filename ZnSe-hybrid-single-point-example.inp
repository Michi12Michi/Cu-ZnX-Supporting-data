!&EXT_RESTART
! RESTART_FILE_NAME ./ZnO-KPBE0-1.restart
!&END EXT_RESTART
&FORCE_EVAL
  METHOD         Quickstep
  &DFT
            BASIS_SET_FILE_NAME /g100_work/IscrC_MHP-DEF/BASIS/BASIS_MOLOPT.txt
            BASIS_SET_FILE_NAME /g100_work/IscrC_MHP-DEF/ZnOSSe/BASIS_MOLOPT_UZH.txt
            !POTENTIAL_FILE_NAME /g100_work/IscrC_MHP-DEF/BASIS/GTH_POTENTIALS.txt
            POTENTIAL_FILE_NAME /g100_work/IscrC_MHP-DEF/ZnOSSe/POTENTIAL_UZH.txt
            BASIS_SET_FILE_NAME /g100_work/IscrC_MHP-DEF/BASIS/BASIS_ADMM_MOLOPT.txt
            BASIS_SET_FILE_NAME /g100_work/IscrC_MHP-DEF/BASIS/BASIS_ADMM.txt
            WFN_RESTART_FILE_NAME ./ZnSe-RESTART.wfn
  CHARGE +2
   UKS T
  MULTIPLICITY 2
    &MGRID
      CUTOFF       600
      REL_CUTOFF    60
    &END MGRID
    &QS
    &END QS
    &SCF
      SCF_GUESS  RESTART
      MAX_SCF    50
      &OT
        MINIMIZER       CG
        PRECONDITIONER  FULL_SINGLE_INVERSE
      &END
      &OUTER_SCF
        MAX_SCF  50
      &END
    &END SCF
    &AUXILIARY_DENSITY_MATRIX_METHOD
        METHOD BASIS_PROJECTION
        ADMM_PURIFICATION_METHOD MO_DIAG
     &END AUXILIARY_DENSITY_MATRIX_METHOD
    &PRINT
       &MO_CUBES
        NHOMO 1
       NLUMO 1
        WRITE_CUBE 
!         &EACH
!          MD 50
!         &END EACH
      &END MO_CUBES
!      &V_HARTREE_CUBE
!      &END
       &E_DENSITY_CUBE
       &END
       &HIRSHFELD
           FILENAME hirsh
       &END
   &PDOS
         FILENAME pdos
         NLUMO 100
     &END PDOS
    &END PRINT
&XC
 &XC_FUNCTIONAL
  &PBE
!   PARAMETRIZATION REVPBE
   SCALE_X 0.75
   SCALE_C 1.0
 &END PBE
 &PBE_HOLE_T_C_LR
 CUTOFF_RADIUS 5.5
 SCALE_X 0.25
 &END PBE_HOLE_T_C_LR
&END XC_FUNCTIONAL
&HF
&SCREENING
EPS_SCHWARZ 1.0E-6
SCREEN_ON_INITIAL_P FALSE
&END SCREENING
&INTERACTION_POTENTIAL
POTENTIAL_TYPE TRUNCATED
CUTOFF_RADIUS 5.5
T_C_G_DATA  /g100_work/IscrC_MHP-DEF/t_c_g.dat
&END INTERACTION_POTENTIAL
&MEMORY
MAX_MEMORY 3600
EPS_STORAGE_SCALING 0.1
&END MEMORY
FRACTION 0.25
&END HF
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 17.0028 17.0028 17.0028
      ALPHA_BETA_GAMMA 90.0 90.0 90.0
    &END CELL
    &TOPOLOGY
      COORD_FILE_NAME  CuZnSe.xyz
      COORD_FILE_FORMAT xyz
    &END TOPOLOGY
    &KIND Zn
      BASIS_SET DZVP-MOLOPT-PBE0-GTH-q12
      BASIS_SET AUX_FIT cFIT9
      POTENTIAL GTH-PBE0-q12
    &END KIND
    &KIND O
      BASIS_SET DZVP-MOLOPT-PBE0-GTH-q6
      BASIS_SET AUX_FIT cFIT3
      POTENTIAL GTH-PBE0-q6
    &END KIND
    &KIND Cu
      BASIS_SET DZVP-MOLOPT-PBE0-GTH-q11
      BASIS_SET AUX_FIT cFIT11
      POTENTIAL GTH-PBE0-q11
    &END KIND
    &KIND S
      BASIS_SET DZVP-MOLOPT-PBE0-GTH-q6
      BASIS_SET AUX_FIT cFIT3
      POTENTIAL GTH-PBE0-q6
    &END KIND
    &KIND Se
      BASIS_SET DZVP-MOLOPT-PBE0-GTH-q6
      BASIS_SET AUX_FIT cFIT6
      POTENTIAL GTH-PBE0-q6
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&MOTION
&GEO_OPT
  OPTIMIZER BFGS
    &BFGS
     TRUST_RADIUS 0.1
    &END
&END GEO_OPT
&END MOTION
&GLOBAL
  PROJECT      ZnSe
  RUN_TYPE     WFN_OPT
  PRINT_LEVEL  LOW
&END GLOBAL

